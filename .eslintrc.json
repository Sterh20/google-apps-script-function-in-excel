{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "semi": "off",
      "no-unexpected-multiline": "error",
      "multiline-ternary": ["error", "never"]
    }
}
